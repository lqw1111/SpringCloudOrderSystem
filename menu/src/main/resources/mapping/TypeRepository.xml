<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.aispringcloud.repository.TypeRepository">

    <select id="findById" parameterType="int" resultType="Type">
        select * from orderingsystem.t_type where id = #{id}
    </select>

    <select id="findAll" resultType="Type">
        select * from orderingsystem.t_type
    </select>

</mapper>